    <string name="charHero1NameEng">Dental Perfectionman</string><string name="charHero1NameSpn">Dental Perfectionman</string>
    <string name="charHero1BioEng">Dental Perfectionman has a passion for helping people.  
        Bad dental hygiene can lead to many other issues.  
        His goal is to educate and fight against bad dental hygiene.  
        He fights against Dr. Racuza and his minions to help people take care of themselves and their teeth.</string>
    <string name="charHero1BioSpn">I NEED A TRANSLATION</string>
    <string name="charHero1RepEng">He\'s a dentist</string>
    <string name="charHero1RepSpn">I NEED A TRANSLATION</string>
    
    <string name="charHero2NameEng">Perla Blanca</string><string name="charHero2NameSpn">Perla Blanca</string>
